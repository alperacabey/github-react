{
  "name": "github-react",
  "homepage": "https://alperacabey.github.io/github-react/",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@fontsource/roboto": "^4.5.1",
    "@octokit/auth-token": "^2.5.0",
    "@octokit/request-error": "^2.1.0",
    "@octokit/rest": "^18.12.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dotenv": "^0.1.3",
    "react-paginate": "^8.0.1",
    "react-redux": "^7.2.6",
    "react-scripts": "4.0.3",
    "react-tooltip": "^4.2.21",
    "recharts": "^2.1.6",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --verbose",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t alperacabey/github-react:latest .",
    "docker:run": "docker run -it -p 3000:3000 github-react",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "autoprefixer": "^9.8.8",
    "enzyme": "^3.11.0",
    "postcss": "^7.0.39",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  },
  "react-dotenv": {
    "whitelist": [
      "REACT_APP_ACCESS_TOKEN"
    ]
  }
}